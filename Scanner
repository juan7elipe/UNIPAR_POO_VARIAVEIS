import java.util.Scanner;

class HelloWorld {
    public static void main(String[] args) {
    
    String leia = "Juan";
    String sobrenome = "Felipe";
    int id;
    
    
    Scanner leiaNome = new Scanner(System.in);
    Scanner leiaSobrenome = new Scanner(System.in);
    

    System.out.println("Digite seu nome: ");
    leia = leiaNome.next(); 

    System.out.println("Seu nome e " + leia);    
       
    System.out.println("Digite seu sobrenome: ");
    sobrenome = leiaSobrenome.next(); 
    
    System.out.println("Seu sobrenome e " + sobrenome);  
    
    
        
    }
}
